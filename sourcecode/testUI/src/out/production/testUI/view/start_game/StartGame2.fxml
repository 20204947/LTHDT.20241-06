<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="500.0" styleClass="anchor-pane-start"
            stylesheets="@StartGameStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.StartGame2Controller">
   <Text fill="#ff6666" layoutX="188.0" layoutY="230.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name"
         styleClass="player1-text">
      <font>
         <Font name="MV Boli" size="18.0"/>
      </font>
   </Text>
   <Text fill="#ff6666" layoutX="188.0" layoutY="151.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID"
         styleClass="player1-text">
      <font>
         <Font name="MV Boli" size="18.0"/>
      </font>
   </Text>
   <Button fx:id="quitButton" layoutX="212.0" layoutY="350.0" mnemonicParsing="false" onAction="#quit" prefHeight="30.0"
           prefWidth="100.0" text="QUIT" styleClass="quit-button">
      <font>
         <Font name="MV Boli" size="12.0"/>
      </font>
   </Button>
   <Button fx:id="startButton" layoutX="212.0" layoutY="302.0" mnemonicParsing="false" onAction="#startGame"
           prefHeight="30.0" prefWidth="100.0" text="START" styleClass="start-button">
      <font>
         <Font name="MV Boli" size="12.0"/>
      </font>
   </Button>
   <Text fill="#ff6666" layoutX="225.0" layoutY="111.0" strokeType="OUTSIDE" strokeWidth="0.0" text="PLAYER"
         styleClass="player1-text">
      <font>
         <Font name="MV Boli" size="18.0"/>
      </font>
   </Text>
   <TextField fx:id="id" layoutX="188.0" layoutY="164.0"/>
   <TextField fx:id="name" layoutX="188.0" layoutY="243.0"/>
   <Text fill="#00cc00" layoutX="142.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="START GAME"
         styleClass="start-text">
      <font>
         <Font name="MV Boli" size="36.0"/>
      </font>
   </Text>
</AnchorPane>
