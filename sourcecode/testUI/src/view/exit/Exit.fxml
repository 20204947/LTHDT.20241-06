<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #e0ffff;" stylesheets="@ExitStyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ExitController">
   <children>
      <Text fill="#ff00fe" layoutX="1.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Do you want to exit ?" textAlignment="CENTER" wrappingWidth="300.0">
         <font>
            <Font name="Jokerman" size="18.0" />
         </font>
      </Text>
      <Button fx:id="noButton" layoutX="60.0" layoutY="85.0" mnemonicParsing="false" onAction="#no" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: RED;" text="NO" textFill="WHITE">
         <font>
            <Font name="Jokerman" size="12.0" />
         </font></Button>
      <Button fx:id="yesButton" layoutX="172.0" layoutY="85.0" mnemonicParsing="false" onAction="#yes" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: BLUE;" text="YES" textFill="WHITE">
         <font>
            <Font name="Jokerman" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
