<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #e0ffff;" stylesheets="@ExitStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testUI.ExitController">
   <children>
      <Text fill="#ff00fe" layoutX="1.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Do you want to exit ?" textAlignment="CENTER" wrappingWidth="300.0">
         <font>
            <Font name="Jokerman" size="18.0" />
         </font>
      </Text>
      <Button fx:id="noButton" layoutX="60.0" layoutY="85.0" mnemonicParsing="false" onAction="#no" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: RED;" text="NO">
         <font>
            <Font name="Jokerman" size="12.0" />
         </font></Button>
      <Button fx:id="yesButton" layoutX="172.0" layoutY="85.0" mnemonicParsing="false" onAction="#yes" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: BLUE;" text="YES">
         <font>
            <Font name="Jokerman" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
